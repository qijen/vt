	</Debugger>

	<material name="Default">
		<settings DynamicFriction="1.0"/>
		<settings StaticFriction="1000.0" />
		<settings Restitution="0.0" />
		<settings DynamicFrictionV="1.0" />
		<settings StaticFrictionV="1.0" />
		<settings DirOfAnisotropy="0.0,0.0,0.0" />
